    <script type="text/javascript">
        $(function () {
            $(".btn-add-basket").click(function () {
                var uid = $(this).attr("uid");
                $.ajax({
                    method: "POST",
                    url: "/AddBasket.aspx",
                    data: {
                        UidArticle: uid
                    }
                }).done(function (msg) {
                    if (msg == "True") {
                        location.reload();
                    }
                });
            });
            $(".btn-remove-basket").click(function () {
                var uid = $(this).attr("uid");
                $.ajax({
                    method: "POST",
                    url: "/RemoveBasket.aspx",
                    data: {
                        UidArticle: uid
                    }
                }).done(function (msg) {
                    if (msg == "True") {
                        location.reload();
                    }
                });
            });
            $(".btn-clear-basket").click(function () {
                $.ajax({
                    method: "POST",
                    url: "/ClearBasket.aspx",
                }).done(function (msg) {
                    if (msg == "True") {
                        location.reload();
                    }
                });
            });
        });
    </script>
    <!--------------------------------------------->
